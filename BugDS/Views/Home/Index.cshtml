                <div class="row">

                    <div class="col-md-3 hidden-xs wow bounceInLeft">
                        <h3>Customers</h3>
                    </div>

                    <div class="col-md-9 wow bounceInRight">

                        <div id="owl-clients" class="owl-carousel">

                            <div class="owl-item">
