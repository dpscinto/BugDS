        <!-- Footer start -->

        <footer id="footer">

            <div class="container">

                <div class="row">

                    <div class="col-md-12">
                        <p class="copy">
                            © 2016 Dan Scinto, All Rights Reserved.
                        </p>
                    </div>

                </div><!-- .row -->

            </div><!-- .container -->

        </footer>

        <!-- Footer end -->
    </div><!-- .wrapper -->
</body>
</html>

